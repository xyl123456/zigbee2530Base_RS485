    457                  {
    458                    CommonApp_PermitJoiningRequest(PERMIT_JOIN_TIMEOUT);
    459                  }
    460                }
    461              }
    462            }
    463            else if (keysPush[0] == HAL_KEY_SHORT_PUSH)
    464            {
    465          #ifdef KEY_PUSH_PORT_1_BUTTON
    466              DeviceCtrl_HandlePort1Keys(keys, keyCounts);
    467          #endif
    468            }
    469          }
