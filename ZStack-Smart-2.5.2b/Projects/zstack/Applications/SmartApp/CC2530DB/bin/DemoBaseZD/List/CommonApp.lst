    444          void CommonApp_ProcessZDOMsgs( zdoIncomingMsg_t *inMsg )
    445          {
    446            switch ( inMsg->clusterID )
    447            {
    448              case End_Device_Bind_rsp:
    449                if ( ZDO_ParseBindRsp( inMsg ) == ZSuccess )
    450                {
    451                  // Light LED
    452                  //HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    453                }
    454          #if defined( BLINK_LEDS )
    455                else
    456                {
    457                  // Flash LED to show failure
    458                  //HalLedSet ( HAL_LED_4, HAL_LED_MODE_FLASH );
    459                }
    460          #endif
    461                break;
    462          
    463              case Match_Desc_rsp:
    464                {
    465                  ZDO_ActiveEndpointRsp_t *pRsp = ZDO_ParseEPListRsp( inMsg );
    466                  if ( pRsp )
    467                  {
    468                    if ( pRsp->status == ZSuccess && pRsp->cnt )
    469                    {
    470                      CommonApp_DstAddr.addrMode = (afAddrMode_t)Addr16Bit;
    471                      CommonApp_DstAddr.addr.shortAddr = pRsp->nwkAddr;
    472                      // Take the first endpoint, Can be changed to search through endpoints
    473                      CommonApp_DstAddr.endPoint = pRsp->epList[0];
    474          
    475                      // Light LED
    476                      //HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    477                    }
    478                    osal_mem_free( pRsp );
    479                  }
    480                }
    481                break;
    482            }
