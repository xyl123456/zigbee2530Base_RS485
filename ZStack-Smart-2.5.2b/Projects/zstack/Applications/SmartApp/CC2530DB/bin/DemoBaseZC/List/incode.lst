     80          //16 进制字符转化为十进制
     81          uint8 ctox(uint8 src)
     82          {
     83          	uint8 temp = 0;
     84          
     85          	if(src>='0' && src<='9')
     86          		temp = src-'0';
     87          	else if(src>='a' && src<='f')
     88          		temp = src-'a'+10;
     89          	else if(src>='A' && src<='F')
     90          		temp = src-'A'+10;
     91          
     92          	return temp;
     93          }
