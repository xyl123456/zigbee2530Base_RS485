    306          /*********************************************************************
    307           * @fn      CommonApp_PowerOnFactorySetting
    308           *
    309           * @brief   reset to factory setting by power on off
    310           *
    311           * @param   network status
    312           *
    313           * @return  none
    314           */
    315          void CommonApp_PowerOnFactorySetting(devStates_t status)
    316          {
    317            if(isFirstState)
    318            {
    319          	if ( osal_nv_item_init( ZCD_NV_POWER_SETTING_COUNT,
    320                                        sizeof(powerSetCount),
    321                                        &powerSetCount ) == ZSUCCESS )
    322          	{
    323          		osal_nv_read( ZCD_NV_POWER_SETTING_COUNT,
    324          		            	0,
    325          		                sizeof( powerSetCount ),
    326          		                &powerSetCount);
    327          	}
    328          
    329          	set_user_event(CommonApp_TaskID, POWERSETTING_COUNT_EVT, CommonApp_PowerSettingCountCB, 
    330            		POWERSETTING_TIMEOUT, TIMER_ONE_EXECUTION, NULL);
    331            }
    332          	
    333            if(status == DEV_ZB_COORD || status == DEV_ROUTER 
    334            		|| status == DEV_END_DEVICE)
    335            {
    336          	hasConn = 1;
    337          	
    338          	if(powerSetCount != 0)
    339          	{
    340          		powerSetCount = 0;
    341          		if ( osal_nv_item_init( ZCD_NV_POWER_SETTING_COUNT,
    342                                        sizeof(powerSetCount),
    343                                        &powerSetCount ) == ZSUCCESS )
    344          		{
    345          		    osal_nv_write( ZCD_NV_POWER_SETTING_COUNT,
    346          		                  0,
    347          		                  sizeof( powerSetCount ),
    348          		                  &powerSetCount);
    349          		}
    350          	}
    351          
    352          	return;
    353            }
    354            
    355            if ((zgReadStartupOptions()&ZCD_STARTOPT_DEFAULT_NETWORK_STATE)
    356            		|| hasConn || isNotTimeOut)
    357            {
    358              return;
    359            }
    360          
    361            if(!isPowerSetCount)
    362            {
    363            	isPowerSetCount = 1;
    364            	powerSetCount++;
    365            	if ( osal_nv_item_init( ZCD_NV_POWER_SETTING_COUNT,
    366                                      sizeof(powerSetCount),
    367                                      &powerSetCount ) == ZSUCCESS )
    368            	{
    369          	  osal_nv_write( ZCD_NV_POWER_SETTING_COUNT,
    370          		             0,
    371          		             sizeof( powerSetCount ),
    372          		             &powerSetCount);
    373            	}
    374          
    375          	if(powerSetCount <= POWERSETTING_COUNT)
    376            	{
    377          	  return;
    378            	}
    379          
    380          #if defined(HOLD_INIT_AUTHENTICATION)
    381              if(devState != DEV_HOLD)
    382              {
    383                HalLedBlink ( HAL_LED_4, 0, 50, 100 );
    384                devStates_t tStates;
    385                if (ZSUCCESS == osal_nv_item_init( 
    386                                    ZCD_NV_NWK_HOLD_STARTUP, sizeof(tStates),  &tStates))
    387                {
    388                  tStates = DEV_HOLD;
    389                  osal_nv_write(ZCD_NV_NWK_HOLD_STARTUP, 0, sizeof(tStates),  &tStates);
    390                }
    391          
    392                zgWriteStartupOptions(ZG_STARTUP_SET, ZCD_STARTOPT_DEFAULT_NETWORK_STATE);
    393                WatchDogEnable( WDTIMX );
    394              }
    395          #endif
    396            }
    397          }
