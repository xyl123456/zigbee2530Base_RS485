    310          /**************************************************************************************************
    311           * @fn          zmain_dev_info
    312           *
    313           * @brief       This displays the IEEE (MSB to LSB) on the LCD.
    314           *
    315           * input parameters
    316           *
    317           * None.
    318           *
    319           * output parameters
    320           *
    321           * None.
    322           *
    323           * @return      None.
    324           **************************************************************************************************
    325           */
    326          static void zmain_dev_info(void)
    327          {
    328          #ifdef LCD_SUPPORTED
    329            uint8 i;
    330            uint8 *xad;
    331            uint8 lcd_buf[Z_EXTADDR_LEN*2+1];
    332          
    333            // Display the extended address.
    334            xad = aExtendedAddress + Z_EXTADDR_LEN - 1;
    335          
    336            for (i = 0; i < Z_EXTADDR_LEN*2; xad--)
    337            {
    338              uint8 ch;
    339              ch = (*xad >> 4) & 0x0F;
    340              lcd_buf[i++] = ch + (( ch < 10 ) ? '0' : '7');
    341              ch = *xad & 0x0F;
    342              lcd_buf[i++] = ch + (( ch < 10 ) ? '0' : '7');
    343            }
    344            lcd_buf[Z_EXTADDR_LEN*2] = '\0';
    345            HalLcdWriteString( "IEEE: ", HAL_LCD_LINE_1 );
    346            HalLcdWriteString( (char*)lcd_buf, HAL_LCD_LINE_2 );
    347          #endif
    348          }
